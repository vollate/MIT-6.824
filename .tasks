# vim: set fenc=utf-8 ft=taskini:
# see: https://github.com/skywind3000/asynctasks.vim/wiki/Task-Config

[project-init]
command=cmake -B build -GNinja
cwd=<root>

[project-build]
command=cmake --build build
cwd=<root>
errorformat=%. %#--> %f:%l:%c

[project-run]
command=go test -run 3A
cwd=<root>/src/raft
output=terminal

